# Config
alias zconfig='$EDITOR $HOME/.zshrc'

# Shell
alias cd="z"
alias ls="eza --icons"
alias cat="bat"
alias grep="rg"
alias less="moar"
alias cl="clear"

# Arch
alias pi="yay -S"
alias pu="yay -Syu"
alias pr="sudo pacman -Rns"
alias pc="sudo pacman -Scc"

# Misc
alias c="code -r"
alias nf="clear && neofetch"
alias names="names | tr '-' '_'"
